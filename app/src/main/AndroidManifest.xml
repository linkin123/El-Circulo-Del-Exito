        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.menu.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.registroAlumno.AltaAlumnoActivity" />
        <activity android:name=".ui.listaAlumnos.ListaAlumnosActivity" />
        <activity android:name=".ui.reporteAlumno.AsesoriasAlumnoActivity" />
        <activity android:name=".ui.opcionesAlumno.OpcionesActivity" />
        <activity android:name=".ui.altaPaquete.AltaPaqueteActivity" />
        <activity android:name=".ui.registroAlumno.AltaAlumnActivity" />
        <activity android:name=".ui.transicionAlumno.SeleccionaAlumnoActivity" />
